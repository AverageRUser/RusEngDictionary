<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <Company>$(Authors)</Company>
    <Authors>$(AssemblyName)</Authors>
    <AssemblyVersion>0.1.4.1</AssemblyVersion>
    <FileVersion>0.1.4.1</FileVersion>
    <Copyright>AMPrograms</Copyright>
    <Version>0.1.4-rc1</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="icons\correct.png" />
    <None Remove="icons\error.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MySqlConnector" Version="2.3.1" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="icons\correct.png" />
    <Resource Include="icons\error.png" />
  </ItemGroup>

</Project>
