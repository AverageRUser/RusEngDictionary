<Window x:Class="RusEngDictionary.WindowDatabaseEntry"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
xmlns:local="clr-namespace:RusEngDictionary" xmlns:col="clr-namespace:System.Collections;assembly=System.Runtime.Extensions"
mc:Ignorable="d"
        Title="WindowDatabaseEntry" Height="220" Width="420">
    <Grid>
        <Button Content="OK" HorizontalAlignment="Left" Margin="305,150,0,0" VerticalAlignment="Top" Width="85" Command="{Binding OpenCommand}"/>
        <TextBox HorizontalAlignment="Left" Margin="9,53,0,0" TextWrapping="Wrap" Text="{Binding Password}" VerticalAlignment="Top" Width="190" />
        <Label Content="Введите пароль от базы данных:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" />

    </Grid>
</Window>
